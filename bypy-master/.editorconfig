# EditorConfig: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
indent_style = tab
indent_size = tab

# Set default charset
[*.py]
charset = utf-8
tab_width = 4

[*.json]
tab_width = 4
